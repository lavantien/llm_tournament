openai-api-base: https://glhf.chat/api/openai/v1
api-key:
  - openai=
  - anthropic=
  - gemini=
  - xai=
  - mistraL=
  - codestRAL=
  - cohere=
architect: true
model: gemini/gemini-exp-1206
edit-format: diff
editor-model: gemini/gemini-2.0-flash-exp
editor-edit-format: editor-diff
weak-model: mistral/mistral-large-latest
read:
  - RULES.md
  - PROMPT.md
  - INSTRUCTION.md
  - preliminary_design.md
  - fyne_doc.md
  - fyne_api.html
yes-always: false
map-tokens: 2048
timeout: 300
cache-prompts: true
cache-keepalive-pings: 300
verbose: true
pretty: true
vim: true
editor: vim
multiline: false
code-theme: coffee
show-diffs: true
