openai-api-base: https://glhf.chat/api/openai/v1
api-key:
  - openai=
  - anthropic=
  - gemini=
  - xai=
  - mistraL=
  - codestRAL=
  - cohere=
architect: true
model: mistral/mistral-large-latest
edit-format: diff
editor-model: mistral/mistral-large-latest
editor-edit-format: editor-diff
weak-model: mistral/mistral-small-latest
read:
  - ../RULES.md
  - ../PROMPT.md
  - ../INSTRUCTION.md
  - ../preliminary_design.md
  - ../tech_stack.md
yes-always: false
map-tokens: 2048
timeout: 300
cache-prompts: true
cache-keepalive-pings: 300
verbose: true
pretty: true
vim: true
editor: vim
multiline: false
code-theme: coffee
show-diffs: true
